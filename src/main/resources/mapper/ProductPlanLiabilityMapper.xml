<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="tech.chaosmin.framework.dao.ProductPlanLiabilityDAO">
    <select id="pageExt" resultType="tech.chaosmin.framework.dao.dataobject.ext.ProductPlanLiabilityExt">
        SELECT pp.plan_code, pl.*
        FROM product_plan_liability pl
        left join product_plan pp on pp.id = pl.product_plan_id
        ${ew.customSqlSegment}
    </select>
</mapper> 
